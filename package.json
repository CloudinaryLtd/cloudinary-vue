{
  "name": "cloudinary-vue",
  "version": "1.0.0-beta2",
  "module": "esm/index.js",
  "browser": "esm/index.js",
  "main": "cjs/index.js",
  "unpkg": "dist/cloudinary-vue.min.js",
  "scripts": {
    "build": "npm run styleguide:build && npm run trans && npm run bundle",
    "bundle": "NODE_ENV='production' webpack --env regular && NODE_ENV='production' webpack --env minified",
    "trans": "npm run transC && npm run transM",
    "transC": "MODULE=CJS babel src --out-dir cjs",
    "transM": "MODULE=ES babel src --out-dir esm",
    "lint": "eslint src && eslint tests",
    "test": "npm run lint && JEST=1 jest",
    "styleguide": "node docs-sources/generateDocsLinks.js; STYLEGUIDE=1 vue-styleguidist server",
    "styleguide:build": "node docs-sources/generateDocsLinks.js; STYLEGUIDE=1 vue-styleguidist build",
    "preversion": "npm t && npm run build"
  },
  "dependencies": {
    "cloudinary-core": "^2.6.2",
    "vue": "^2.6.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.3",
    "@vue/babel-preset-jsx": "^1.0.0-beta.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^8.0.5",
    "core-js": "^3.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "glob": "^7.1.3",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jstoxml": "^1.4.4",
    "terser": "^3.17.0",
    "terser-webpack-plugin": "^1.2.3",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-styleguidist": "^3.11.0",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^13.2.2"
  }
}
